# Example Terraform variables file
# Copy this file to terraform.tfvars and fill in your values
# terraform.tfvars should NOT be committed to git (it's in .gitignore)

# Required: Your S3 bucket name (must be globally unique)
bucket_name = "my-unique-website-bucket-12345"

# Optional: AWS region (defaults to us-east-1)
# aws_region = "us-east-1"

# Optional: CloudFront price class (defaults to PriceClass_100)
# Options: PriceClass_100 (US/Canada/Europe), PriceClass_200 (adds Asia/Middle East/Africa), PriceClass_All
# cloudfront_price_class = "PriceClass_100"

# Optional: Enable S3 versioning (defaults to false to minimize costs)
# enable_s3_versioning = false

